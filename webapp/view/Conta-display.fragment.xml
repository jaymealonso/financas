<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core" >

<ColumnListItem>
    <cells>
        <Text text="{contas>_id}" />
        <Text text="{contas>descricao}" />
        <Text text="{contas>numero}" />
        <Text text="{contas>moeda}" />
        <Text text="{contas>tipo_descricao}" />
        <Text text="{contas>count_categ}" />
        <Text text="{contas>count_n_categ}" />        
        <Button icon="sap-icon://table-chart" tooltip="Visão Mensal" type="Transparent" press="onContaVisaoMensalPress" />
        <Button icon="sap-icon://edit"  tooltip="Editar Conta" type="Transparent" press="onContaEditPress" />
        <Button icon="sap-icon://activity-items" tooltip="Ver Lançamentos" type="Transparent" press="onContaLancamentosPress" />
    </cells>
</ColumnListItem>

</core:FragmentDefinition>